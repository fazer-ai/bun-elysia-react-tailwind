{
  "name": "bun-elysia-react-tailwind",
  "version": "0.0.0",
  "private": true,
  "module": "src/index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fazer-ai/fazer-ai-hub.git"
  },
  "scripts": {
    "dev": "bun --hot src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "build": "bun run build.ts",
    "test": "bun test --coverage",
    "test-watch": "bun test --watch",
    "lint": "bun biome check --error-on-warnings",
    "format": "bun biome check --write",
    "build-check": "bunx tsc --noEmit",
    "setup": "bun ./scripts/setup.ts",
    "set-admin": "bun scripts/set-admin.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "i18n:extract": "i18next 'src/client/**/*.{ts,tsx}' --config i18next-parser.config.cjs",
    "check": "bun run lint && bun run build-check && bun run test && bun run i18n:extract",
    "prepare": "husky"
  },
  "dependencies": {
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.5",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/static": "^1.4.7",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bun-plugin-tailwind": "^0.1.2",
    "elysia": "^1.4.19",
    "elysia-rate-limit": "^4.4.2",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "pino-roll": "^4.0.0",
    "react": "^19",
    "react-dom": "^19",
    "react-i18next": "^16.5.0",
    "react-router": "^7.11.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@happy-dom/global-registrator": "^20.0.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "husky": "^9.1.7",
    "i18next-parser": "^9.3.0",
    "pino-caller": "^4.0.0",
    "prisma": "^7.2.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
